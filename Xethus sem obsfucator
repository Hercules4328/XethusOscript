local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/NIGHT163626/ScriptHUB/refs/heads/main/open%20sourcezada.txt'))()
local Player = game.Players.LocalPlayer

-- Configurações da Janela Principal
local Window = OrionLib:MakeWindow({
    Name = "Xethus Hub | Universal BR", 
    SearchBar = { Default = 'Pesquise', ClearTextOnFocus = true }, 
    HidePremium = false, 
    SaveConfig = true, 
    ConfigFolder = "Xethus",
    IntroText = "Xethus Hub",
    IntroIcon = "rbxassetid://73200509058998"
})

-- Função Auxiliar para Adicionar Botões
local function AddScriptButton(tab, name, url)
    tab:AddButton({
        Name = name,
        Callback = function()
            loadstring(game:HttpGet(url))()
        end
    })
end
-- [ Aba: canal do Youtube e Executores]
local DiscordTab = Window:MakeTab({
    Name = "Painel ",
    Icon = "rbxassetid://11819163504",
    PremiumOnly = false
})

-- Mensagem e botão para copiar o link do Discord
DiscordTab:AddLabel("Ultima atualização em 09/12/24")
DiscordTab:AddButton({
    Name = "Comunidade do Discord ",
    Callback = function()
        setclipboard("https://discord.gg/GMDGeYEVHW") -- Link do Discord
        OrionLib:MakeNotification({
            Name = "Link do Discord Copiado",
            Content = "O link do Discord foi copiado!",
            Image = "rbxassetid://5013032505",
            Time = 5
        })
    end
})
DiscordTab:AddLabel("Executores Atualizado!")
DiscordTab:AddButton({
    Name = "Executores Atualizado ( site )",
    Callback = function()
        setclipboard("https://beacons.ai/ethus/mobile") -- Link do Beacons
        OrionLib:MakeNotification({
            Name = "Link dos executores foi Copiado",
            Content = "O link foi copiado!",
            Image = "rbxassetid://11819163504",
            Time = 5
        })
    end
})

